<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/background_gradient_primary">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/default_spacing_20"
            android:orientation="vertical"
            android:layout_marginTop="@dimen/button_margin_50"
            android:layout_marginLeft="@dimen/button_margin_30"
            style="@style/title_white_h5"
            android:text="@string/team_my_levels"
            android:textSize="20sp"
            android:fontFamily="@font/futura_bold"
            android:shadowColor="@color/secondary_color_400"
            android:shadowRadius="5"
            android:shadowDy="5"
            android:shadowDx="0"/>

        <RelativeLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/title"
            android:paddingBottom="@dimen/margin_70">
            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/button_margin_20"
                android:layout_marginRight="@dimen/button_margin_20" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/default_spacing_20"
                        android:background="@drawable/empty_rounded_border"
                        android:orientation="vertical"
                        android:padding="@dimen/default_spacing_20">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <TextView
                                android:id="@+id/userlevelText"
                                style="@style/title_white_h5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/default_spacing_20"
                                android:fontFamily="@font/futura_bold"
                                android:textSize="20sp"
                                android:text="@string/LEVEL" />
                            <TextView
                                android:id="@+id/usernameText"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_alignBaseline="@id/userlevelText"
                                style="@style/body_white_1"
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_regular"
                                android:text="@string/username"/>
                        </RelativeLayout>



                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/default_spacing_20">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center">

                                <View
                                    android:id="@+id/icon1"
                                    android:layout_width="@dimen/icon_size_40"
                                    android:layout_height="@dimen/icon_size_40"
                                    android:background="@drawable/empty_circle"
                                    android:padding="4dp"
                                    android:scaleType="fitCenter"/>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center">

                                <View
                                    android:id="@+id/icon2"
                                    android:layout_width="@dimen/icon_size_40"
                                    android:layout_height="@dimen/icon_size_40"
                                    android:background="@drawable/empty_circle"
                                    android:padding="4dp"
                                    android:scaleType="fitCenter"/>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center">

                                <ImageView
                                    android:id="@+id/icon3"
                                    android:layout_width="@dimen/margin_50"
                                    android:layout_height="@dimen/margin_50"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/icons8_bot_white_48" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center">

                                <View
                                    android:id="@+id/icon4"
                                    android:layout_width="@dimen/icon_size_40"
                                    android:layout_height="@dimen/icon_size_40"
                                    android:background="@drawable/empty_circle"
                                    android:padding="4dp"
                                    android:scaleType="fitCenter"/>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center">

                                <View
                                    android:id="@+id/icon5"
                                    android:layout_width="@dimen/icon_size_40"
                                    android:layout_height="@dimen/icon_size_40"
                                    android:background="@drawable/empty_circle"
                                    android:padding="4dp"
                                    android:scaleType="fitCenter"/>
                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/default_spacing_10"
                            android:orientation="horizontal">

                            <TextView
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_extrabold"
                                android:textColor="@color/text_color_white_100"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/team_level" />

                            <TextView
                                android:id="@+id/rankText"
                                style="@style/body_white_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="13"
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_regular"
                                android:gravity="right"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/default_spacing_10"
                            android:orientation="horizontal">

                            <TextView
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_extrabold"
                                android:textColor="@color/text_color_white_100"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/team_my_points" />

                            <TextView
                                android:id="@+id/myPointsText"
                                style="@style/body_white_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="28103"
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_regular"
                                android:gravity="right"/>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/leaderboardButton"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/default_spacing_10"
                            android:orientation="horizontal"
                            android:clickable="true">

                            <TextView
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_extrabold"
                                android:textColor="@color/text_color_white_100"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/team_leaderboard" />

                            <TextView
                                style="@style/body_white_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="&gt;"
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_regular"
                                android:gravity="right"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/achievementsButton"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:clickable="true">

                            <TextView
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_extrabold"
                                android:textColor="@color/text_color_white_100"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/team_my_achievements" />

                            <TextView
                                style="@style/body_white_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="&gt;"
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_regular"
                                android:gravity="right"/>
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/empty_rounded_border"
                        android:orientation="vertical"
                        android:padding="@dimen/default_spacing_20"
                        android:layout_marginBottom="@dimen/button_margin_20">

                        <LinearLayout
                            android:id="@+id/ly_myTeam"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/default_spacing_20"
                            android:orientation="horizontal">

                            <TextView
                                style="@style/title_white_h5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/MYTEAM" />

                            <TextView
                                android:id="@+id/teamnameText"
                                style="@style/body_white_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/team_name"
                                android:gravity="right"/>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ly_teamButton"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/default_spacing_10"
                            android:orientation="horizontal"
                            android:clickable="true">

                            <TextView
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_extrabold"
                                android:textColor="@color/text_color_white_100"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/team_team" />

                            <TextView
                                style="@style/body_white_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="&gt;"
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_regular"
                                android:gravity="right"/>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/discoverButton"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/default_spacing_10"
                            android:orientation="horizontal"
                            android:clickable="true">

                            <TextView
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_extrabold"
                                android:textColor="@color/text_color_white_100"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/team_discover" />

                            <TextView
                                style="@style/body_white_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="&gt;"
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_regular"
                                android:gravity="right"/>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/inviteButton"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/default_spacing_10"
                            android:orientation="horizontal"
                            android:clickable="true">

                            <TextView
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_extrabold"
                                android:textColor="@color/text_color_white_100"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/team_invite_friends" />

                            <TextView
                                style="@style/body_white_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="&gt;"
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_regular"
                                android:gravity="right"/>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ly_fav"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/empty_rounded_border"
                        android:orientation="vertical"
                        android:padding="@dimen/default_spacing_20"
                        android:layout_marginBottom="@dimen/button_margin_20">

                        <LinearLayout
                            android:id="@+id/ly_fav_title"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/default_spacing_20"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_fav_title"
                                style="@style/title_white_h5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/FAVORITES" />

                            <TextView
                                android:id="@+id/tv_fav_title_counter"
                                style="@style/body_white_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="6"
                                android:gravity="right"/>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ly_fav_edit"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/default_spacing_10"
                            android:orientation="horizontal"
                            android:clickable="true">

                            <TextView
                                android:id="@+id/tv_fav_edit_t"
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_extrabold"
                                android:textColor="@color/text_color_white_100"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/EDITFAV" />

                            <TextView
                                android:id="@+id/tv_fav_edit_c"
                                style="@style/body_white_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="&gt;"
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_regular"
                                android:gravity="right"/>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ly_assiation"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/empty_rounded_border"
                        android:orientation="vertical"
                        android:padding="@dimen/default_spacing_20">

                        <LinearLayout
                            android:id="@+id/ly_ass_title"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/default_spacing_20"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_ass_title"
                                style="@style/title_white_h5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/ASSOCIATION" />

                            <TextView
                                android:id="@+id/tv_ass_title_counter"
                                style="@style/body_white_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="6"
                                android:gravity="right"/>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ly_ass_edit"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/default_spacing_10"
                            android:orientation="horizontal"
                            android:clickable="true">

                            <TextView
                                android:id="@+id/tv_ass_edit_t"
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_extrabold"
                                android:textColor="@color/text_color_white_100"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/EDITASS" />

                            <TextView
                                android:id="@+id/tv_ass_edit_c"
                                style="@style/body_white_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="&gt;"
                                android:textSize="14sp"
                                android:fontFamily="@font/proxima_nova_regular"
                                android:gravity="right"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </ScrollView>


        </RelativeLayout>
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:layout_gravity="bottom"
        android:background="@drawable/shape_drawable"/>
    <com.google.android.material.bottomappbar.BottomAppBar
        android:id="@+id/bar"
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:layout_gravity="bottom"
        android:backgroundTint="@color/bottomapppbartint"
        app:fabCradleDiameter="70dp"
        app:hideOnScroll="false">

        <LinearLayout
            android:id="@+id/tabBar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/logButton"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:src="@drawable/icons8_menu_48"
                android:padding="@dimen/image_icon_padding_10"
                android:scaleType="fitCenter"
                android:clickable="false"
                android:contentDescription="TODO" />

            <ImageView
                android:id="@+id/settingButton"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:src="@drawable/ic_dial"
                android:padding="@dimen/image_icon_padding_10"
                android:scaleType="fitCenter"
                android:clickable="false"/>

            <View
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"/>

            <ImageView
                android:id="@+id/teamButton"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:src="@drawable/icons8_people_48"
                android:padding="@dimen/image_icon_padding_10"
                android:scaleType="fitCenter"
                android:clickable="false"/>

            <ImageView
                android:id="@+id/searchButton"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:src="@drawable/icons8_search_24"
                android:padding="@dimen/image_icon_padding_10"
                android:scaleType="fitCenter"
                android:clickable="false"/>

        </LinearLayout>


    </com.google.android.material.bottomappbar.BottomAppBar>
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/text_color_white_100"
        app:srcCompat="@drawable/icon_chevron_up"
        app:layout_anchor="@id/bar"/>

</androidx.coordinatorlayout.widget.CoordinatorLayout>